{
	"compilerOptions": {
	  "declaration": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "module": "CommonJS",
	  "noImplicitAny": true,
	  "removeComments": true,
	  "skipDefaultLibCheck": true,
	  "strict": true,
	  "target": "ES2016"
	},
	"exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"]
  }
  